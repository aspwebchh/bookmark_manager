{
  "name": "My First Extension",
  "version": "0.1",
  "manifest_version": 2,
  "description": "The first extension that I made.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": [ "http://121.40.60.241/*" ],
      "css": [ ],
      "js": [ "scripts/jquery-1.9.1.min.js", "app.js" ]
    }
  ]
}